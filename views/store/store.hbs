{{!< store }}
<div class="row">
		<div class="col-lg-12">
				<h1 class="page-header">
						Store DashBoard
				</h1>
				<ol class="breadcrumb">
						<li>
								<i class="fa fa-dashboard"></i>  <a href="/store">Dashboard</a>
						</li>
				</ol>
				<hr>
				<table class="table">
					<thread>
						<tr>
							<th>State</th>
							<th>Date & Time</th>
							<th>Store</th>
							<th>Method</th>
							<th>Items</th>
						</tr>
					</thread>
					<tbody id="content">
						{{#each orders}}
							<tr>
								<td>
									{{this.value.state}}
								</td>
								<td>
									{{this.value.dateTime}}
								</td>
								<td>
									{{this.value.location}}
								</td>
								<td>
									{{this.value.method}}
								</td>
								<td>
									<div class="dropdown"><button class="btn btn-default dropdown-toggle"
										type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
										View Items<span class="caret"></span></button><ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
											{{#each this.value.items}}
												<li>{{this}}</li>
											{{/each}}
										</ul></div>
								</td>
							</tr>
						{{/each}}
					</tbody>
				</table>
		</div>
</div>
